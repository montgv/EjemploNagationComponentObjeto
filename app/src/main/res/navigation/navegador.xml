<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navegador"
    app:startDestination="@id/finalFragment">

    <fragment
        android:id="@+id/inicioFragment"
        android:name="com.example.ejemplonagationcomponentobjeto.Fragmentos.InicioFragment"
        android:label="fragment_inicio"
        tools:layout="@layout/fragment_inicio" >
        <action
            android:id="@+id/action_inicioFragment_to_finalFragment"
            app:destination="@id/finalFragment" />
    </fragment>
    <fragment
        android:id="@+id/finalFragment"
        android:name="com.example.ejemplonagationcomponentobjeto.Fragmentos.FinalFragment"
        android:label="fragment_final"
        tools:layout="@layout/fragment_final" >
        <action
            android:id="@+id/action_finalFragment_to_inicioFragment"
            app:destination="@id/inicioFragment" />
        <argument
            android:name="Persona"
            app:argType="com.example.ejemplonagationcomponentobjeto.Persona" />
    </fragment>
</navigation>